{
    "workbench.colorCustomizations": {
        "activityBar.background": "#2C2E2E",
        "titleBar.activeBackground": "#3D4140",
        "titleBar.activeForeground": "#FCFCFC"
    },
    "rust-analyzer.checkOnSave.overrideCommand": [
        "cargo",
        "check",
        "--target",
        "aarch64-linux-android",
        "--all-features",
        "--message-format=json",
    ],
    "rust-analyzer.cargo.buildScripts.overrideCommand": [
        "cargo",
        "check",
        "--target",
        "aarch64-linux-android",
        "--all-features",
        "--message-format=json",
    ],
    "rust-analyzer.cargo.target": "aarch64-linux-android",
    "rust-analyzer.trace.server": "verbose",
    "rust-analyzer.server.extraEnv": {
        "ANDROID_NDK_API_LEVEL": 30,
        "ANDROID_SDK_VERSION": 30,
        "ANDROID_API_LEVEL": 30,
        "ANDROID_BUILD_TOOLS_VERSION": "33.0.1",
        "ANDROID_SDK_HOME": "${env:HOME}/Android/Sdk",
        "ANDROID_HOME": "${env:ANDROID_SDK_HOME}/build-tools/${env:ANDROID_BUILD_TOOLS_VERSION}",
        "ANDROID_NDK_HOME": "${env:ANDROID_SDK_HOME}/ndk/25.2.9519653",
        "ANDROID_NDK": "${env:ANDROID_NDK_HOME}",
    },
    "rust-analyzer.cargo.buildScripts.invocationLocation": "workspace",
    "rust-analyzer.cargo.features": "all"
}